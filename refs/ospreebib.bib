	Journal = {Scientia Horticulturae},
	Note = {Times Cited: 0 Heide, Ola M. Sonsteby, Anita 0},
	Pages = {228-232},
	Title = {Simultaneous dormancy induction interferes with short day floral induction in black currant (Ribes nigrum L.)},
	Type = {Journal Article},
